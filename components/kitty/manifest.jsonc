{
  "name": "kitty",
  "description": "Terminal. CuteðŸ˜¼, fast, and do more than just run commands!",
  "builds": {
    "arch": ["sudo pacman -S kitty"],
    "cachyos": ["sudo pacman -S kitty"],
    "fedora": ["sudo dnf install kitty -y"],
    "nobara": ["sudo dnf install kitty -y"]
  },
  "configs": [
    {
      "from": "./src",
      "to": "~/.config/kitty",
      "method": "symlink",
      "force": true,
      "tags": ["*"]
    }
  ],
  "cleanup": ["rm -rf ~/.config/kitty"]
}
